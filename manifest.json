{
    "manifest_version": 2,
    "name": "Meet Attendance Taker",
    "description": "Quickly take attendance from google meet",
    "version": "1.0.1",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Meet Attendance Taker"
    },
    "permissions": ["activeTab", "declarativeContent", "storage", "contextMenus"],
    "background": {
        "scripts": ["content.js"]
    }
}